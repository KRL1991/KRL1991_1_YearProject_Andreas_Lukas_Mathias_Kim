<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.x1_year_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KRL1991_1_YearProject_Andreas_Lukas_Mathias_Kim">
        <activity android:name=".Confirmation"
            android:label="Bekræftelse"/>
        <activity android:name=".ListOfChosenItems"
            android:label="Kurv"/>
        <activity android:name=".InfoSite"
            android:label="Kontakt Information"/>
        <activity
            android:name=".AboutUs"
            android:label="Om os" />
        <activity
            android:name=".ChosenWall"
            android:label="Angiv Mål" />
        <activity android:name=".Inspiration"
            android:label="Inspiration"/>
        <activity android:name=".Catalog"
            android:label="Katalog"/>
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StartScreen"
            android:label="Hjem">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>