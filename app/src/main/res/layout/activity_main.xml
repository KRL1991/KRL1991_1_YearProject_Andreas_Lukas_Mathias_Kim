<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".StartScreen">

    <Button
        android:id="@+id/btncatalog"
        android:layout_width="195dp"
        android:layout_height="67dp"
        android:layout_marginBottom="5dp"
        android:text="Katalog"
        app:backgroundTint="#BF8E55"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.569" />

    <Button
        android:id="@+id/btninspiration"
        android:layout_width="195dp"
        android:layout_height="67dp"
        android:layout_marginTop="20dp"
        android:text="Inspiration"
        app:backgroundTint="#BF8E55"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btncatalog" />

    <Button
        android:id="@+id/btnaboutus"
        android:layout_width="195dp"
        android:layout_height="67dp"
        android:layout_marginTop="20dp"
        android:text="Om os"
        app:backgroundTint="#BF8E55"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btninspiration" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="313dp"
        android:layout_height="252dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/first" />

</androidx.constraintlayout.widget.ConstraintLayout>